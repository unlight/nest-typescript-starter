{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "0.0.0-dev",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "postinstall": "patch-package",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "node -r dotenv/config node_modules/@nestjs/cli/bin/nest.js start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "eslint": "eslint \"{src,test}/**/*.ts\"",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "tslint:fix": "node node_modules/tslint/bin/tslint -p tsconfig.json --fix",
    "lint:fix": "npm run tslint:fix || true && npm run eslint:fix",
    "tscheck": "echo tscheck... && tsc --noEmit --incremental false",
    "tscheck:w": "npm run tscheck -- --watch",
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:cov": "jest --runInBand --coverage --verbose",
    "test:d": "node --inspect-brk node_modules/jest/bin/jest --runInBand --testMatch \"<rootDir>/**/*.spec.ts\"",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.js",
    "commit": "node node_modules/cz-customizable/standalone.js",
    "cmlint": "sh Taskfile commit_lint",
    "test:m": "stryker run",
    "migrate:save": "node -r dotenv/config node_modules/prisma2/build/index.js migrate save --experimental",
    "migrate:up": "node -r dotenv/config node_modules/prisma2/build/index.js migrate up --experimental",
    "prisma:w": "node -r dotenv/config node_modules/prisma2/build/index.js generate --watch",
    "prisma:generate": "node -r dotenv/config node_modules/prisma2/build/index.js generate",
    "prisma:studio": "node -r dotenv/config node_modules/prisma2/build/index.js studio --experimental",
    "dev:update": "updtr --out --to latest --test \"npm test:r\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits",
      "pre-push": "npm run test",
      "commit-msg": "git-branch-is -r \"^(release|master|develop)\" && commitlint -E HUSKY_GIT_PARAMS || echo \"Ignoring commitlint error...\""
    }
  },
  "dependencies": {
    "@nestjs/common": "^6.11.8",
    "@nestjs/core": "^6.11.8",
    "@nestjs/graphql": "^6.6.2",
    "@nestjs/platform-express": "^6.11.8",
    "apollo-server-express": "^2.10.1",
    "dataloader": "^2.0.0",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "type-graphql": "^0.17.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.2",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^6.11.8",
    "@stryker-mutator/core": "2.5.0",
    "@stryker-mutator/html-reporter": "^2.5.0",
    "@stryker-mutator/jest-runner": "^2.5.0",
    "@stryker-mutator/typescript": "^2.5.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "cz-customizable": "^6.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^16.1.1",
    "eslint-plugin-wix-editor": "^3.0.0",
    "git-branch-is": "^3.1.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "patch-package": "^6.2.1",
    "precise-commits": "^1.0.2",
    "prettier": "^1.19.1",
    "prisma2": "^2.0.0-alpha.843",
    "stryker-cli": "^1.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0",
    "tslint-clean-code": "^0.2.10",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-etc": "^1.10.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-sonarts": "^1.9.0",
    "typegraphql-prisma": "^0.1.5",
    "typescript": "^3.8.3",
    "watchexec-bin": "^1.0.0"
  }
}
